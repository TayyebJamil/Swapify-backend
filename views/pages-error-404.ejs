<main>
  <div class="container">
    <section
      class="section error-404 min-vh-100 d-flex flex-column align-items-center justify-content-center"
    >
      <h1>404</h1>
      <h2>The page you are looking for doesn't exist.</h2>
      <img
        src="/assets/img/not-found.svg"
        class="img-fluid py-5"
        alt="Page Not Found"
      />
    </section>
  </div>
</main>
<script>
  if (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream) {
    window.location.href = 'https://apps.apple.com/us/app/facebook/id284882215'
  }

  // Check if user is using an Android device
  if (/Android/.test(navigator.userAgent)) {
    window.location.href =
      'https://play.google.com/store/apps/details?id=com.facebook.katana&hl=en&gl=US&pli=1'
  }
</script>
